{
  "Serilog": {
    "MinimumLevel": "Information",
    "Override": {
      "Microsoft.AspNetCore.Routing.EndpointMiddleware": "Warning",
      "Microsoft.AspNetCore.Hosting.Diagnostics": "Warning"
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [
      "FromLogContext"
    ]
  },
  "Resilience": {
    "Enabled": true
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=shop-postgresql;Database=shop;Username=postgres;Password=adminpass"
  }
}
